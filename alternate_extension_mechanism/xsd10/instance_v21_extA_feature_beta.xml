<?xml version="1.0" encoding="UTF-8"?>
<kml
  xmlns="http://www.opengis.net/kml/2"
  xmlns:kml="http://www.opengis.net/kml/2"
  xmlns:kml21="http://www.opengis.net/kml/2.1"
  xmlns:extA="http://www.opengis.net/kml/2/ext/extensionA"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xsi:schemaLocation=
    "http://www.opengis.net/kml/2 ./standard_v21_core.xsd
     http://www.opengis.net/kml/2.1 ./accepted_direct_swg_additions_to_v21.xsd
     http://www.opengis.net/kml/2/ext/extensionA draft_extA_feature_beta_extending_v21.xsd"
     version="2.1.0">
  <!-- Instance of KML 2.1 (including Organization A's accepted extensions) plus Organization A's extensions not accepted into 2.1. -->
  <Kml20Container>
    <Kml20Feature>
      <name>Untitled Placemark</name>
      <kml20FeatureChild>anySimpleType goes here</kml20FeatureChild> <!-- Per 2.1 schema, anySimpleType is now valid -->
      <extA:extensionToKml20Feature>something</extA:extensionToKml20Feature>
    </Kml20Feature>
    <extA:Kml20ExtensionFeatureAlpha>
      <name>foo</name>
      <extA:kml20ExtensionFeatureAlphaChild>something</extA:kml20ExtensionFeatureAlphaChild>
    </extA:Kml20ExtensionFeatureAlpha>
    <extA:Kml20ExtensionFeatureBeta>  <!-- This unaccepted KML 2.1 feature is once again valid with the proper schema defs. -->
      <name>foo</name>
      <extA:kml20ExtensionFeatureBetaChild>something</extA:kml20ExtensionFeatureBetaChild>
    </extA:Kml20ExtensionFeatureBeta>
    <kml21:Kml21Feature>
      <name>foo</name>
      <kml21:kml21FeatureChild>asdasd</kml21:kml21FeatureChild>
    </kml21:Kml21Feature>
    <extA:extensionToKml20Container>something</extA:extensionToKml20Container>   <!-- This unaccepted KML 2.1 feature is once again valid with the proper schema defs. -->
  </Kml20Container>
</kml>
