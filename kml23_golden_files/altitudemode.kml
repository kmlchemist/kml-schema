<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" version="2.3">
 <!--
    In order to preserve backwards compatibility, KML
    provides both <altitudeMode> and <seaFloorAltitudeMode>, that way
    old clients that don't understand the seafloor/bathymetry data
    can still display something reasonable.
   -->
 <Document>
    <Placemark>
      <name>clamped to ground/seafloor 2.3</name>
      <Point>
        <extrude>1</extrude>
        <altitudeMode>clampToGround</altitudeMode>
        <seaFloorAltitudeMode>clampToSeaFloor</seaFloorAltitudeMode>
        <coordinates>0,0,10000</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>relative to ground/seafloor 2.3</name>
      <Point>
        <extrude>1</extrude>
        <altitudeMode>relativeToGround</altitudeMode>
        <seaFloorAltitudeMode>relativeToSeaFloor</seaFloorAltitudeMode>
        <coordinates>0,0,10000</coordinates>
      </Point>
    </Placemark>
  </Document>
</kml>