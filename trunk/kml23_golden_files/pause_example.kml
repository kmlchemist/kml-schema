<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" version="2.3">

<Tour>
  <Playlist>

    <FlyTo>
      <duration>5</duration>
      <flyToMode>bounce</flyToMode>
      <LookAt>
        <longitude>-79.387</longitude>
        <latitude>43.643</latitude>
        <altitude>0</altitude>
        <heading>-172.3</heading>
        <tilt>10</tilt>
        <range>1200</range>
        <altitudeMode>relativeToGround</altitudeMode>
      </LookAt>
    </FlyTo>

    <SoundCue>
      <href>
        http://code.google.com/apis/kml/documentation/kmlfiles/cntowerfacts.mp3
      </href>   <!-- this file is ten seconds long -->
    </SoundCue>

    <Wait>
      <duration>5</duration>
    </Wait>

    <TourControl>
      <playMode>pause</playMode>
    </TourControl>

    <Wait>
      <duration>5</duration>
    </Wait>

  </Playlist>
</Tour>
</kml>
