    substitutionGroup="kml:AbstractObjectGroup"/>

  <!--
    TODO: Eval this idea: abstract bounding box type from which both
    AbstractLatLonBoxType and LatLonQuad inherit from. This also leaves
    door open for bounding boxes according to tile id's and such.
  -->
  <element name="AbstractBoundingBoxGroup" type="kml:AbstractBoundingBoxType" abstract="true"
    substitutionGroup="kml:AbstractObjectGroup"/>
  <complexType name="AbstractBoundingBoxType" abstract="true">
    <complexContent>
      <extension base="kml:AbstractObjectType">
        <all>
          <element ref="kml:AbstractBoundingBoxSimpleExtensionGroup" minOccurs="0"
            maxOccurs="unbounded"/>
          <element ref="kml:AbstractBoundingBoxObjectExtensionGroup" minOccurs="0"
            maxOccurs="unbounded"/>
        </all>
      </extension>
    </complexContent>
  </complexType>
  <element name="AbstractBoundingBoxSimpleExtensionGroup" abstract="true" type="anySimpleType"/>
  <element name="AbstractBoundingBoxObjectExtensionGroup" abstract="true"
    substitutionGroup="kml:AbstractObjectGroup"/>

  <element name="LatLonQuad" type="kml:LatLonQuadType"
    substitutionGroup="kml:AbstractBoundingBoxGroup"/>
