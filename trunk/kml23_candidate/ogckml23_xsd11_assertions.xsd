  <complexType name="IconLinkType" final="#all">
    <complexContent>
      <extension base="kml:LinkType">
        <all>
          <!--
            TODO: IconStyle/Icon and AbstractOverlay/Icon are different but
            IconStyle/Icon also needs to support gridX/Y/W/H!
          -->
          <element ref="kml:gridX" minOccurs="0"/>
          <element ref="kml:gridY" minOccurs="0"/>
          <element ref="kml:gridW" minOccurs="0"/>
          <element ref="kml:gridH" minOccurs="0"/>
          <element ref="kml:IconLinkTypeSimpleExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
          <element ref="kml:IconLinkTypeObjectExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
          <element ref="kml:CrossIconPropertiesSimpleExtensionGroup" minOccurs="0"
            maxOccurs="unbounded"/>
          <element ref="kml:CrossIconPropertiesObjectExtensionGroup" minOccurs="0"
            maxOccurs="unbounded"/>
        </all>
      </extension>
    </complexContent>
  </complexType>
  <element name="IconLinkTypeSimpleExtensionGroup" abstract="true" type="anySimpleType"/>
  <element name="IconLinkTypeObjectExtensionGroup" abstract="true"
    substitutionGroup="kml:AbstractObjectGroup"/>

  <!-- 
    TODO: Had to make multiple subsitution groups for choice/all replacement
    This could be bad. See: http://lists.w3.org/Archives/Public/xmlschema-dev/2012Aug/0034.html
  -->
  <element name="Link" type="kml:LinkType"
    substitutionGroup="kml:AbstractObjectGroup kml:AbstractLinkGroup"/>
  <element name="Url" type="kml:LinkType"
    substitutionGroup="kml:AbstractObjectGroup kml:AbstractLinkGroup">
    <annotation>
      <documentation>Url deprecated in 2.2</documentation>
    </annotation>
  </element>
  <!-- 
    TODO: We can't make LinkType final, because IconLinkType extends it! 
  -->
  <complexType name="LinkType">
    <complexContent>
      <extension base="kml:BasicLinkType">
        <all>
          <element ref="kml:refreshMode" minOccurs="0"/>
          <element ref="kml:refreshInterval" minOccurs="0"/>
          <element ref="kml:viewRefreshMode" minOccurs="0"/>
          <element ref="kml:viewRefreshTime" minOccurs="0"/>
          <element ref="kml:viewBoundScale" minOccurs="0"/>
          <element ref="kml:viewFormat" minOccurs="0"/>
          <element ref="kml:httpQuery" minOccurs="0"/>
          <element ref="kml:LinkSimpleExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
          <element ref="kml:LinkObjectExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
        </all>
      </extension>
    </complexContent>
  </complexType>
  <element name="LinkSimpleExtensionGroup" abstract="true" type="anySimpleType"/>
  <element name="LinkObjectExtensionGroup" abstract="true"
    substitutionGroup="kml:AbstractObjectGroup"/>

  <element name="MultiGeometry" type="kml:MultiGeometryType"
    substitutionGroup="kml:AbstractGeometryGroup"/>
  <complexType name="MultiGeometryType" final="#all">
    <complexContent>
      <extension base="kml:AbstractGeometryType">
        <all>
          <element ref="kml:AbstractGeometryGroup" minOccurs="0" maxOccurs="unbounded"/>
          <element ref="kml:MultiGeometrySimpleExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
          <element ref="kml:MultiGeometryObjectExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
        </all>
      </extension>
    </complexContent>
  </complexType>
  <element name="MultiGeometrySimpleExtensionGroup" abstract="true" type="anySimpleType"/>
  <element name="MultiGeometryObjectExtensionGroup" abstract="true"
    substitutionGroup="kml:AbstractObjectGroup"/>

  <element name="Point" type="kml:PointType" substitutionGroup="kml:AbstractGeometryGroup"/>
  <complexType name="PointType" final="#all">
