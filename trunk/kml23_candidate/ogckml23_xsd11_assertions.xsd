          <element ref="kml:seaFloorAltitudeMode" minOccurs="0"/>
          <element ref="kml:coordinates" minOccurs="0"/>
          <element ref="kml:PointSimpleExtensionGroup" minOccurs="0" maxOccurs="unbounded"/>
